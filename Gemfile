source "https://rubygems.org"

gem "jekyll-sass-converter", "~> 3.0"


# 如果您的网站使用了 Jekyll 的某个主题，可以在这里添加
# 例如，如果您使用的是 "minima" 主题，则添加：
gem "minima", "~> 2.5"

# 如果您需要其他 Jekyll 插件，也可以在这里添加
# 例如，添加 Jekyll 的 feed 插件：
gem "jekyll-feed", "~> 0.12"

#tzinfo 是一个 Ruby 库，用于处理时区转换，它是 Jekyll 的一个依赖
gem 'tzinfo', '~> 2.0'
gem 'tzinfo-data'
